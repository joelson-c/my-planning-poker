{
  "name": "@planningpoker/domain-models",
  "version": "1.0.0",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "Provides domain models definition for voting application",
  "exports": {
    "./realtime/*": "./dist-ts/*.ts",
    "./realtime/base/*": "./dist-ts/base/*.ts",
    "./voting/*": "./src/voting/*.ts",
    "./bindings/*": "./src/bindings/*.ts"
  },
  "scripts": {
    "lint": "eslint",
    "build": "protoc --experimental_allow_proto3_optional -Isrc/protos --go_out=dist_go/domain --go_opt=paths=source_relative --plugin=../../node_modules/.bin/protoc-gen-es --es_out=dist-ts --es_opt target=ts ./src/protos/*.proto ./src/protos/base/*.proto"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.1.0",
    "superstruct": "^2.0.2"
  },
  "devDependencies": {
    "@bufbuild/protoc-gen-es": "^2.1.0",
    "@planningpoker/eslint-config-planning-poker": "^0.0.0",
    "@planningpoker/tsconfig": "^0.0.0",
    "typescript": "^5.1.6"
  }
}
