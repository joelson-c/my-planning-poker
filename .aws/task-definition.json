{
    "family": "socket-backend-node-task",
    "containerDefinitions": [
        {
            "name": "socket-node",
            "image": "004189553810.dkr.ecr.sa-east-1.amazonaws.com/my-planning-backend:latest",
            "cpu": "256",
            "memory": "512",
            "portMappings": [
                {
                    "name": "socket-node-443-tcp",
                    "containerPort": 3000,
                    "hostPort": 443,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "ulimits": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "/ecs/socket-backend-node-task",
                    "awslogs-region": "sa-east-1",
                    "awslogs-stream-prefix": "ecs"
                },
                "secretOptions": []
            }
        }
    ],
    "executionRoleArn": "arn:aws:iam::004189553810:role/ecsTaskExecutionRole",
    "networkMode": "bridge",
    "requiresCompatibilities": [
        "EC2"
    ],
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}
